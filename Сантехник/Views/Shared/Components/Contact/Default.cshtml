@model List<ContactListForUI>

<!-- ======= Contact Section ======= -->
<section id="contact" class="contact">
    <div class="container">

        <div class="section-title">
            <span>Contact</span>
            <h2>Contact</h2>
            <p>Our branches located:</p>
        </div>

        <div class="row">

            <div class="d-flex align-items-stretch">
                <div class="info">

                    @foreach (var item in Model)
                    {
                        <div class="address">
                            <i class="bi bi-geo-alt"></i>
                            <h4>Location:</h4>
                            <p>@item.Location</p>
                        </div>

                        <div class="email">
                            <i class="bi bi-envelope"></i>
                            <h4>Email:</h4>
                            <p>@item.Email</p>
                        </div>

                        <div class="phone">
                            <i class="bi bi-phone"></i>
                            <h4>Call:</h4>
                            <p>@item.Call</p>
                        </div>

                        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d35919.98614324236!2d37.58943556148124!3d55.75851724903864!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x414ab5a270f9bde7%3A0xc4a140b68cc1b348!2sStroitel&#39;nyy%20Dvor!5e0!3m2!1sen!2sru!4v1720873386130!5m2!1sen!2sru" frameborder="0" style="border:0; width: 100%; height: 500px;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
                    }

                </div>

            </div>



        </div>

    </div>
</section><!-- End Contact Section -->