@model List<TeamListForUI>

<!-- ======= Team Section ======= -->
<section id="team" class="team section-bg">
    <div class="container">

        <div class="section-title">
            <span>Team</span>
            <h2>Team</h2>
            <p>Meet our crew...</p>
        </div>

        <div class="row">
            <div class="col-lg-4 col-md-6 d-flex align-items-stretch">

                @foreach(var item in Model)
                {
                <div class="member">
                    <img src="~/images/@item.FileName" width="150" height="200" alt="">
                    <h4>@item.FullName</h4>
                    <span>@item.Title</span>
                    <div class="social">
                        <a href="@item.Twitter"><i class="bi bi-twitter"></i></a>
                        <a href="@item.Facebook"><i class="bi bi-facebook"></i></a>
                        <a href="@item.Instagram"><i class="bi bi-instagram"></i></a>
                        <a href="@item.Linkedin"><i class="bi bi-linkedin"></i></a>
                    </div>
                </div>
                }
            </div>

            

        </div>

    </div>
</section><!-- End Team Section -->